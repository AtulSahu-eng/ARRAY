package p2;

public class Sum_Of_Elements {
public static void main(String []a)
{
int []arr={1,2,3,4,5,8,9,6,7,10};//Declaration and Initialization of Array

int sum=0;//Initial value of sum is 0
//till condition will be true ,loop is executed
for(int i=0;i<arr.length;i++)
{
sum=sum+arr[i];	//sum variable perform the operation and store the value itself
}
System.out.println("Sum of the Elements :"+sum);//printing the total sum of elements
}
}
