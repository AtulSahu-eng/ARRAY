

public class MatchStringValue {
public static void main(String[] args) {
	String []arr= {"String","Story","Dancing","Writing","Poet","Scripting","Distance"};

	int count=0;
	
	for(int i=0;i<arr.length;i++)
	{
		//it is used to find the ing 
		String var=arr[i].substring(arr[i].length()-3);
	

	//check ing contains ing
		if(var.contains("ing"))
		{
			System.out.println(arr[i]);
			count++;
		}
	}
	System.out.println("total count of value is :"+count);

	}
}

